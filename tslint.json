{
    "rules": {
        "ban": [
            true,
            [
                "_",
                "extend"
            ],
            [
                "_",
                "isNull"
            ],
            [
                "_",
                "isDefined"
            ]
        ],
        "class-name": true,
        "comment-format": [
            true,
            "check-space",
        ],
        "curly": false,
        "eofline": false,
        "forin": true,
        "indent": [
            true,
            "spaces",
            4
        ],
        "interface-name": true,
        "jsdoc-format": true,
        "label-position": true,
        "max-line-length": [
            true,
            540
        ],
        "no-arg": true,
        "no-bitwise": false,
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-string-literal": false,
        "no-trailing-whitespace": true,
        "no-unused-expression": true,
        "no-unused-variable": true,
        "no-use-before-declare": true,
        "one-line": [
            false,
            "check-open-brace",
            "check-catch",
            "check-else",
            "check-whitespace"
        ],
        "quotemark": [
            true,
            "double"
        ],
        "radix": true,
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "typedef": [
            true,
            "call-signature",
            "parameter",
            "property-declaration"
        ],
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace"
            },
            {
                "index-signature": "space"
            }
        ],
        "variable-name": false,
        "whitespace": [
            false,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ]
    }
}